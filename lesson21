#include <stdio.h>
#include <math.h>

//kullanıcıdan 3 sayı al ve en büyük sayıyı yazdır.

int main(){

    int num1;
    int num2;
    int num3;
    int highNum=0;

    printf("Please enter 3 numbers.");
    scanf("%d %d %d", &num1,&num2,&num3);

    if(num1>num2){
        highNum=num1;
    } else if(num3>highNum){
        highNum=num3;
    }
    printf("The highest number is: %d",highNum);
}
